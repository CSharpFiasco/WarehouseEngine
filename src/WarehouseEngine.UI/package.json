{
  "name": "warehouse-engine.ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --host=localhost --port 4201",
    "start:vs": "ng serve --host=127.0.0.1 --port 4201",
    "build": "ng build",
    "build:watch": "ng build --watch",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:watch": "ng test --watch",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/warehouse-engine.ui/stats.json",
    "prettify": "prettier --write \"./**/*.{js,jsx,ts,tsx,json}\"",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.7",
    "@angular/cdk": "^20.1.6",
    "@angular/common": "^20.1.7",
    "@angular/compiler": "^20.1.7",
    "@angular/core": "^20.1.7",
    "@angular/forms": "^20.3.16",
    "@angular/material": "^20.2.14",
    "@angular/platform-browser": "^20.1.7",
    "@angular/platform-browser-dynamic": "^20.3.16",
    "@angular/router": "^20.3.16",
    "@ngrx/effects": "^20.1.0",
    "@ngrx/operators": "^20.1.0",
    "@ngrx/store": "^20.0.0",
    "@ngrx/store-devtools": "^20.1.0",
    "d3": "^7.9.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.15",
    "@angular/cli": "^21.1.1",
    "@angular/compiler-cli": "^20.3.16",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.9.1",
    "@types/d3": "^7.4.3",
    "@types/jasmine": "^5.1.15",
    "@web/test-runner": "^0.20.2",
    "angular-eslint": "^20.7.0",
    "eslint": "^9.39.2",
    "eslint-plugin-unused-imports": "^4.3.0",
    "jasmine-core": "^5.13.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "prettier": "^3.8.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "webpack-bundle-analyzer": "^4.10.2"
