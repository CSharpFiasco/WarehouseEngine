    "@eslint/js": "^9.9.1",
    "@types/d3": "^7.4.3",
    "@types/jasmine": "^5.1.9",
    "@web/test-runner": "^0.20.2",
    "angular-eslint": "^19.8.1",
    "eslint": "^9.33.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jasmine-core": "^5.9.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
